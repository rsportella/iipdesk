<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="inteligence_in_productionPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>Entitys.Usuario</class>
    <class>Entitys.Pessoa</class>
    <class>Entitys.Campo</class>
    <class>Entitys.Permissao</class>
    <class>Entitys.Diretiva</class>
    <class>Entitys.Auditoria</class>
    <class>Entitys.Logradouro</class>
    <class>Entitys.PermissaoView</class>
    <class>Entitys.PermissoesView</class>
    <class>Entitys.Tela</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:postgresql://localhost:5432/iip"/>
      <property name="javax.persistence.jdbc.user" value="postgres"/>
      <property name="javax.persistence.jdbc.driver" value="org.postgresql.Driver"/>
      <property name="javax.persistence.jdbc.password" value="postgres"/>
    </properties>
  </persistence-unit>
</persistence>
